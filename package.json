    "grpc_tools_node_protoc_ts": "2.5.4",
    "minimist": "1.2.0",
    "mustache": "3.0.1",
    "pkg-up": "3.1.0",
    "which": "1.3.1"
  },
  "devDependencies": {
    "@types/minimist": "1.2.0",
    "@types/mustache": "0.8.32",
    "@types/node": "12.6.9",
    "@types/which": "1.3.1",
    "husky": "3.0.2",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "typescript": "3.5.3"
  },
  "bin": {
